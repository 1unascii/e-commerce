{
    "name": "joseph/tommys_tees",
    "type": "project",
    "require": {
        "cweagans/composer-patches": "^1.6.0",
        "drupal/adminimal_admin_toolbar": "^1.7",
        "drupal/adminimal_theme": "^1.3",
        "drupal/bootstrap": "^3.11",
        "drupal/business": "^1.7",
        "drupal/commerce": "^2.7",
        "drupal/commerce_authnet": "^1.0@RC",
        "drupal/commerce_license": "^2.0@alpha",
        "drupal/commerce_paypal": "^1.0@beta",
        "drupal/commerce_product": "^2.7",
        "drupal/commerce_recurring": "^1.0@beta",
        "drupal/commerce_reports": "^1.0@beta",
        "drupal/commerce_stock": "1.x-dev",
        "drupal/commerce_stripe": "^1.0@beta",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.5",
        "drupal/ctools": "^3.0",
        "drupal/date": "1.x-dev",
        "drupal/devel": "^1.2",
        "drupal/drush": "^6.0@RC",
        "drupal/entityreference": "1.x-dev",
        "drupal/field_collection": "^1.0@alpha",
        "drupal/field_group": "^1.0",
        "drupal/field_permissions": "^1.0@RC",
        "drupal/field_tools": "^1.0@alpha",
        "drupal/field_validation": "^1.0@alpha",
        "drupal/google_analytics": "^2.2",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/interval": "^1.1",
        "drupal/libraries": "^3.0@alpha",
        "drupal/mayo": "^1.3",
        "drupal/metatag": "^1.5",
        "drupal/module_filter": "^3.1",
        "drupal/panels": "^4.3",
        "drupal/rules": "^3.0@alpha",
        "drupal/seo_checklist": "^4.1",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drupal/zen": "^7.0@alpha",
        "drush/drush": "~8.0|^9.0.0-beta8",
        "hussainweb/drupal-composer-helper": "^1.0"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsstream": "~1.2",
        "phpunit/phpunit": ">=4.8.28 <5",
        "symfony/css-selector": "~2.8"
    },
    "authors": [
        {
            "name": "Joseph Earl Weiner",
            "email": "JWeiner@madisoncollege.edu"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "drupal-composer-helper": {
            "web-prefix": "web"
        },
        "enable-patching": true,
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ]
        }
    },
    "prefer-stable": true,
    "conflict": {
        "drupal/drupal": "*"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
